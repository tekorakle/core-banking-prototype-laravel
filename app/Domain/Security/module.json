    "type": "core",
    "dependencies": {
        "required": {
            "finaegis/shared": "^1.0",
            "finaegis/account": "^1.0"
        },
        "optional": {
            "finaegis/compliance": "^1.0",
            "finaegis/key-management": "^1.0"
        }
    },
    "provides": {
